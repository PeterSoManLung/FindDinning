# Data Integration Service Environment Configuration

# Server Configuration
NODE_ENV=development
PORT=3006

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5433/restaurant_db

# Redis Configuration
REDIS_URL=redis://localhost:6379

# External API Keys
DATA_GOV_HK_API_KEY=
OPENRICE_API_KEY=
TRIPADVISOR_API_KEY=
EATIGO_API_KEY=
CHOPE_API_KEY=
FOODPANDA_API_KEY=
BISTROCHAT_API_KEY=
KEETA_API_KEY=

# Data Synchronization Configuration
SYNC_SCHEDULE=0 0 1 * *
MAX_CONCURRENT_EXTRACTIONS=5
EXTRACTION_TIMEOUT_MS=30000
RETRY_ATTEMPTS=3
RETRY_DELAY_MS=5000

# Web Scraping Configuration
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
REQUEST_DELAY_MS=1000
MAX_PAGES_PER_SOURCE=100

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/data-integration.log